                                            <label for="MainProduct" class="control-label">Main Product<span class="required">*</span></label>
                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement: MainProduct">
                                            <input type="text" data-bind="value:MainProduct" class="form-control" id="MainProduct" placeholder="Main Product">
                                        </div>
                                        <div class="col-sm-2">
                                            <label class="control-label" for="MainClient">Main Client<span class="required">*</span></label>
                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement: MainClient">
                                            <input type="text" data-bind="value:MainClient" class="form-control" id="MainClient" placeholder="Main Client">
                                        </div>
