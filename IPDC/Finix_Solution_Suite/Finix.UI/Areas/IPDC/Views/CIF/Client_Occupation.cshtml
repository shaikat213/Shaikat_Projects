                                        <div class="col-sm-3" data-bind="visible:IsGovtPrivateService, validationElement: CompanyRetirementAge">
                                            <input type="text" data-bind="value:CompanyRetirementAge" class="form-control" id="CompanyRetirementAge" placeholder="Company Retirement Age">
                                        </div>
