                                                <select class="form-control" id="EmploymentStatus" data-bind="options:EmployeeStatusList,optionsValue: 'Id', optionsText:'Name',optionsCaption:'--Select--',value:EmploymentStatus "></select>
                                            </div>
                                            <div class="col-sm-1">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group row row_design_detail" data-bind="visible:OccupationType() === 1 || OccupationType() === 2 || OccupationType() === 3">
                                        <div class="col-sm-1">
                                        </div>
