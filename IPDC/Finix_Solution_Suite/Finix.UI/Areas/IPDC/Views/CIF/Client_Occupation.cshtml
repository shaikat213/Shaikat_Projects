                                            <div class="col-sm-2">
                                                <label for="OfficeAddress.AddressLine2" class="control-label">Address Line 2</label>
                                            </div>
                                            <div class="col-sm-3" data-bind="validationElement: OfficeAddress.AddressLine2">
                                                <input type="text" data-bind="value:OfficeAddress.AddressLine2,event:{change: IsOfcAddressChng}" class="form-control" id="OfficeAddress.AddressLine2" placeholder="Line 2">

                                            </div>
                                            <div class="col-sm-1">
                                            </div>
                                        </div>
                                        <div class="form-group row row_design_detail">
                                            <div class="col-sm-1">
                                            </div>
                                            <div class="col-sm-2">
