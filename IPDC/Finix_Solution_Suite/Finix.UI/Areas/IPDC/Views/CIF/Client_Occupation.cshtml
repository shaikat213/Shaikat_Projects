                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement: LegalStatus, visible:OccupationType() === 3">
                                            <select class="form-control" id="LegalStatus" data-bind="options:LegalStatusList,optionsValue: 'Id', optionsText:'Name',optionsCaption:'--Select--',value:LegalStatus"></select>
                                        </div>
                                        <div class="col-sm-1">
                                        </div>
                                    </div>
                                    <div class="form-group row row_design_detail" data-bind="visible:OccupationType() === 3">
