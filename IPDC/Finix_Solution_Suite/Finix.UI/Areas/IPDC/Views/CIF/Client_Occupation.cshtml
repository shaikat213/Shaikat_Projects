                                        <div class="col-sm-2" data-bind="visible: OccupationType() === 3">
                                            <label class="control-label" for="MonthsInCurrentJob">Years in Business<span class="required">*</span></label>
                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement: MonthsInCurrentJob">
                                            <input type="text" data-bind="value:MonthsInCurrentJob, visible: !(OccupationType() === 3)" class="form-control" id="MonthsInCurrentJob" placeholder="Months In Current Job">
                                            <input type="text" data-bind="value:MonthsInCurrentJob, visible: OccupationType() === 3" class="form-control" id="MonthsInCurrentJob" placeholder="Years In Business">
                                        </div>
                                        <div class="col-sm-2">
                                            <label class="control-label" for="NameOfPreviousEmployeer">Name of Previous Employer<span class="required">*</span></label>
                                        </div>
                                        <div class="col-sm-3" data-bind="validationElement: NameOfPreviousEmployeer">
                                            <input type="text" data-bind="value:NameOfPreviousEmployeer" class="form-control" id="NameOfPreviousEmployeer" placeholder="Name Of Previous Employeer">
                                        </div>
                                        <div class="col-sm-1">
                                        </div>
                                    </div>
                                    <div class="form-group row row_design_detail" data-bind="visible:OccupationType() === 1 || OccupationType() === 2 || OccupationType() === 3">
                                        <div class="col-sm-1">
