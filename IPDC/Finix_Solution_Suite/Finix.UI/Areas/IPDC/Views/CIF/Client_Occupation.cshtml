                                            </div>
                                            <div class="col-sm-2">
                                                <label for="OfficeAddress.DistrictId" class="control-label">District<span class="required">*</span></label>
                                            </div>
                                            <div class="col-sm-3" data-bind="validationElement: OfficeAddress.DistrictId">
                                                <select class="form-control" id="OfficeAddress.DistrictId" data-bind="options:OfficeDistrictList,optionsValue: 'Id',optionsText:'Name',optionsCaption:'--Select--', value:OfficeAddress.DistrictId ,event: {change: getOfficeUpzilaByDistrict,IsOfcAddressChng}"></select>
                                            </div>
                                            <div class="col-sm-1">
                                            </div>
                                        </div>
                                        <div class="form-group row row_design_detail">
                                            <div class="col-sm-1">
