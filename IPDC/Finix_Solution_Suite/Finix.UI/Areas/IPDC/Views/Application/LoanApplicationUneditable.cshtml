                                                            <a class="minus" href='#' data-bind='click: $parent.removeOtherSecurities'><span class="glyphicon glyphicon-minus"></span></a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="form-group row row_design">
                                            @*<div class="col-sm-3">
                                                    <label for="OtherSecurity" class="control-label">Other Security</label>
                                                </div>
                                                <div class="col-sm-3" data-bind="validationElement: OtherSecurity ">
                                                    <input type="text" data-bind="value:OtherSecurity" class="form-control" id="OtherSecurity" placeholder="Other Security">
                                                </div>*@
                                            <div class="col-sm-3">
