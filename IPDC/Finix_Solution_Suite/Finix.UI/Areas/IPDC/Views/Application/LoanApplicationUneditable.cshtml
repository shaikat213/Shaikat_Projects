                                            <div class="col-sm-3">
                                                <label for="PayeesName" class="control-label">Payees Name</label>
                                            </div>
                                            <div class="col-sm-3" data-bind="validationElement: PayeesName">
                                                <input type="text" data-bind="value:PayeesName" class="form-control" id="PayeesName" placeholder="Payees Name">
                                            </div>
                                        </div>

                                        <div class="form-group row row_design" data-bind="visible: DisbursementMode()">
                                            <div class="col-sm-3">
                                                <label for="Bank" class="control-label">Bank</label>
                                            </div>
                                            <div class="col-sm-3" data-bind="validationElement: Bank">
                                                <input type="text" data-bind="value:Bank" class="form-control" id="Bank" placeholder="Bank">
                                            </div>
                                            <div class="col-sm-3">
                                                <label for="Branch" class="control-label">Branch</label>
                                            </div>
