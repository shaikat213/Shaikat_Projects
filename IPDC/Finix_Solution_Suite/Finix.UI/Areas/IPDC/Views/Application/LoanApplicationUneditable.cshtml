                                                        <td>
                                                            <a class="plus" href='#' data-bind='click: $parent.addGuarantors'><span class="glyphicon glyphicon-plus"></span></a>

                                                            <a class="minus" href='#' data-bind='click: $parent.removeGuarantors'><span class="glyphicon glyphicon-minus"></span></a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                        @*</div>
                                                </div>
                                            </div>*@
                                    </div>
                                </div>
                            </div>





