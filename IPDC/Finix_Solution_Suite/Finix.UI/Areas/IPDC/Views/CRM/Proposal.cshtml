@{
    ViewBag.Title = "Proposal";
    Layout = "~/Views/Shared/_LayoutjqGrid.cshtml";
}
<link href="~/Areas/IPDC/Content/cif.css" rel="stylesheet" />
<script src="~/Areas/IPDC/Scripts/addressJsViewModel.js"></script>
<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<link href="~/Content/customassets/css/cifIncomeStatement.css" rel="stylesheet" />
<link href="~/Content/customassets/css/application.css" rel="stylesheet" />
<script src="~/Scripts/KoCustomDateTimePicker.js"></script>
<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<script src="~/Scripts/KoCustomDateTimePicker.js"></script>
<script src="~/Scripts/knockout-jqAutocomplete.min.js"></script>
<script src="~/Areas/IPDC/Scripts/CRM/proposal.js"></script>
<style>
    .form-control {
        width: 100% !important;
    }

    .input-group-addon {
        padding: 0 5px;
    }

    .no-padding {
        padding: 0;
    }

    .no-right-padding {
        padding-right: 0;
    }

    .no-left-padding {
        padding-left: 0;
    }

    .pointer {
        cursor: pointer;
    }

    .lineHeightDate {
        line-height: 15px !important;
    }

    tr > td > input {
        min-height: 22px;
    }

    input[type="number"] {
        text-align: right;
    }
    /*.col-sm-1 .input-group{
        float:left !important;
    }*/
</style>
<div class="panel-body" id="proposalCreate">
    <div class="container-fluid">
        <div class="panel-bodycontent">
            <header class="panel-heading bodytitle">
                <h2 class="panel-title font"><b>Proposal</b></h2>
            </header>
            <div class="panel-body collapse in">
                <fieldset>
                    @*<div class="form-group row row_design_detail" style="margin: 0px; font-size: 11px">
                            <a target="_blank" data-bind="attr: { href: Link1, title1: 'PDF' }">PDF</a>
                            <a target="_blank" data-bind="attr: { href: Link2, title2: 'Excel' }">Excel</a>
                            <a target="_blank" data-bind="attr: { href: Link3, title3: 'Word' }">Word</a>
                        </div>*@
                    <div class="form-group row row_design_detail">
                        <div class="col-sm-4">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="control-label" for="ApplicationReceiveDate">Application No.</label>
                                </div>
                                <div class="col-sm-6 no-padding" data-bind="validationElement:ApplicationNo">
                                    <input type="datetime" data-bind="value:ApplicationNo" class="form-control" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="control-label" for="ApplicationReceiveDate">Application Date</label>
                                </div>
                                <div class="col-sm-6 input-group" data-bind="validationElement:ApplicationReceiveDate">
                                    <input type="text" class="form-control" id="ApplicationReceiveDate" data-bind="datepicker:ApplicationReceiveDate, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate:'keyup'" readonly>
                                    <span class="help-block" style="display: none;"></span>
                                    <div class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row row_design_detail">

                        <div class="col-sm-4">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="control-label" for="CRMReceiveDate">CRM Receive Date</label>
                                </div>
                                <div class="col-sm-6 input-group" data-bind="validationElement:CRMReceiveDate">
                                    <input type="text" class="form-control" id="CRMReceiveDate" data-bind="datepicker:CRMReceiveDate, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate:'keyup'" readonly>
                                    <span class="help-block" style="display: none;"></span>
                                    <div class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label class="control-label">Approval Date</label>
                                </div>
                                <div class="col-sm-6 input-group" data-bind="validationElement:ProposalDate">
                                    <input type="text" class="form-control" id="proposalDate" data-bind="datepicker:ProposalDate, datepickerOptions: {format: 'DD/MM/YYYY'}, valueUpdate:'keyup'">
                                    <span class="help-block" style="display: none;"></span>
                                    <div class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </div>
                                    @*<input type="datetime" data-bind="date:ProposalDate" class="form-control">
                                        <span class="help-block" style="display: none;"></span>
                                        <div class="input-group-addon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </div>*@
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label">Branch</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <input class="form-control" type="text" data-bind="value:BranchName" readonly />
                        </div>
                    </div>
                    <div class="form-group row row_design_detail">

                        <div class="col-sm-2">
                            <label class="control-label">RM Name</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <input class="form-control" type="text" data-bind="value:RMName" />
                        </div>
                        <div data-bind="visible: (FacilityType() != 4)">
                            <div class="col-sm-2">
                                <label class="control-label" for="FacilityType">Facility</label>
                            </div>
                            <div class="col-sm-2 no-padding">
                                <input class="form-control" type="text" data-bind="value:FacilityTypeName" readonly />
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label">Authority Level</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <select data-bind="options:AuthorityLevelList, value: ApprovalAuthorityLevel, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'" class="form-control"></select>
                        </div>

                    </div>
                    <div class="form-group row row_design_detail" data-bind="visible: (FacilityType() === 4)">
                        <div class="col-sm-2">
                            <label class="control-label">Processing Fee And Doc Charges Amount</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <input class="form-control" type="text" data-bind="value:ProcessingFeeAndDocChargesAmount" readonly />
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label">Recomended Loan Amount From IPDC</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            @*<input class="form-control" type="text" data-bind="value:RecomendedLoanAmountFromIPDC" readonly />*@
                            <input type="text" data-bind="value: RecomendedLoanAmountCommaSeperated" class="form-control" readonly />
                        </div>
                        <div class="col-sm-2">
                            <label class="control-label" for="LoantoDepositRatio">Loan to Deposit Ratio</label>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <input class="form-control" type="text" id="LoantoDepositRatio" data-bind="value:LoantoDepositRatio, valueUpdate:'keyup'" />
                        </div>
                    </div>
                    @*<div class="form-group row row_design_detail">
                        </div>*@
                    @*<div class="form-group row row_design_detail">
                            <div class="col-sm-3">
                                <label class="control-label">Facility Type</label>
                            </div>
                            <div class="col-sm-9">
                                <select data-bind="options:FacilityTypeList, value: FacilityType, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'" class="form-control"></select>
                            </div>
                        </div>*@

                    <div class="panel-bodycontent" data-bind="visible: (FacilityType() != 4)">
                        <header class="panel-heading bodytitle pointer" data-toggle="collapse" data-target="#loanRelatedInfo">
                            <h2 class="panel-title font"><b>Loan Related Info</b></h2>
                        </header>
                        <div class="panel-body collapse" id="loanRelatedInfo">
                            <div class="form-group row row_design_detail">
                                <div class="col-sm-2">
                                    <label class="control-label" for="AppliedLoanAmount">Applied Amount</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:AppliedLoanAmount" />*@
                                    <input style="text-align:right;"type="number" min="0" data-bind="value:AppliedLoanAmount, valueUpdate:'keyup'" class="form-control hidden" />
                                    <input style="text-align:right;" type="text" data-bind="value: AppliedLoanAmountFormatted" class="form-control" />
                                </div>
                                <div class="col-sm-2">
                                    <label class="control-label" for="AppliedLoanTerm">Term</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" min="0" data-bind="value:AppliedLoanTermApplication" readonly />
                                </div>
                            </div>
                            <div class="form-group row row_design_detail">

                                <div class="col-sm-2">
                                    <label class="control-label" for="InterestRateCard">Card Interest Rate</label>
                                </div>
                                <div class="col-sm-2 no-padding">

                                    <input class="form-control" type="number" min="0" data-bind="value:InterestRateCard" />
                                </div>

                                <div class="col-sm-2">
                                    <label class="control-label" for="ProcessingFeeAndDocChargesCardRate">Processing Fee & Doc. Charges</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" min="0" data-bind="value:ProcessingFeeAndDocChargesCardRate" />
                                </div>

                            </div>
                            <div class="form-group row row_design_detail">
                                <div class="col-sm-2">
                                    <label class="control-label">Loan Amount</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:RecomendedLoanAmountFromIPDC" readonly />*@
                                    <input type="text" data-bind="value: RecomendedLoanAmountCommaSeperated" class="form-control" readonly />
                                </div>

                                <div class="col-sm-2">
                                    <label class="control-label" for="AppliedLoanTerm">Term</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" id="AppliedLoanTerm" min="0" data-bind="value:AppliedLoanTerm" />
                                </div>
                            </div>
                            <div class="form-group row row_design_detail">

                                <div class="col-sm-2">
                                    <label class="control-label">Offered Interest Rate</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" min="0" data-bind="value:InterestRateOffered" />
                                </div>

                                <div class="col-sm-2">
                                    <label class="control-label">Processing Fee & Doc. Charges %</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" min="0" data-bind="value:ProcessingFeeAndDocChargesPercentage" />
                                </div>
                            </div>
                            <div class="form-group row row_design_detail">
                                <div class="col-sm-2">
                                    <label class="control-label" for="CurrentExposureWithIPDC">Current Exposure with IPDC</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" min="0" data-bind="value:CurrentExposureWithIPDC" />
                                </div>
                                <div class="col-sm-2">
                                    <label class="control-label" for="TotalExposureWithIPDC">Total Exposure with IPDC</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:TotalExposureWithIPDC" readonly />*@
                                    <input style="text-align:right;" type="text" data-bind="value: TotalExposureWithIPDCFormatted" class="form-control" readonly />
                                </div>
                                <div class="col-sm-2 col-sm-offset-1">
                                    <a target="_blank" class="btn btn-default" style="width:auto; line-height:.8;" data-bind="attr:{href:  $root.ViewCardRate($data) }">Rate Chart </a>

                                </div>
                            </div>
                            <div class="form-group row row_design_detail">
                                <div class="col-sm-2">
                                    <label class="control-label" for="DBR">DBR</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:DBR" readonly />*@
                                    <input style="text-align:right;" type="text" data-bind="value: DBRFormatted" class="form-control" />
                                </div>
                                <div class="col-sm-2" data-bind="visible: (FacilityType() != 3)">
                                    <label class="control-label" for="LTVonTotalPresentMarketValue">LTV on Total Present Market Value</label>
                                </div>
                                <div class="col-sm-2" data-bind="visible: (FacilityType() === 3)">
                                    <label class="control-label" for="LTVonTotalPresentMarketValue">Times</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    <input class="form-control" type="number" id="LTVonTotalPresentMarketValue" min="0" data-bind="value:LTVonTotalPresentMarketValue" readonly />
                                </div>
                            </div>
                            <div class="form-group row row_design_detail">

                                <div class="col-sm-2">
                                    <label class="control-label">Monthly Income Total</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:TotalMonthlyIncomeConsidered" readonly />*@
                                    <input style="text-align:right;" type="text" data-bind="value: TotalMonthlyIncomeConsideredFormatted" class="form-control" readonly />
                                </div>
                                <div class="col-sm-2">
                                    <label class="control-label" for="FreeCash">Free Cash</label>
                                </div>
                                <div class="col-sm-2 no-padding">
                                    @*<input class="form-control" type="number" min="0" data-bind="value:FreeCash" readonly />*@
                                    <input style="text-align:right;" type="text" data-bind="value: FreeCashFormatted" class="form-control" readonly />
                                </div>
                            </div>
                            <div class="form-group row row_design_detail">
                                <div class="col-sm-4">

                                    <div class="row">
                                        <div class="col-sm-6">
                                            <label class="control-label" for="RateVariance">Rate Variance</label>
                                        </div>
                                        <div class="col-sm-6 no-padding">
                                            <input class="form-control" type="number" min="0" data-bind="value:RateVariance" />
                                        </div>
                                    </div>

                                </div>
                                <div class="col-sm-8">
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <label class="control-label" for="LoanRemarks">Remarks</label>
                                        </div>
                                        <div class="col-sm-8 no-padding">
                                            <textarea class="form-control" data-bind="value:LoanRemarks" style="min-height: 93px;"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="panel-bodycontent">
                        <header class="panel-heading bodytitle pointer" data-toggle="collapse" data-target="#clientProfile">
                            <h2 class="panel-title font"><b>Client Profile</b></h2>
                        </header>
                        <div class="panel-body collapse" id="clientProfile">
                            <div data-bind="foreach:ClientProfiles">
                                <div class="form-group row row_design_detail">
                                    <div class="col-sm-1">
                                        <label class="control-label">Name</label>
                                    </div>
                                    <div class="col-sm-2 no-padding">
                                        <input class="form-control" type="text" data-bind="value:Name" />
                                    </div>
                                    <div class="col-sm-1">
                                        <label class="control-label">Role</label>
                                    </div>
                                    <div class="col-sm-2 no-padding">
                                        <input class="form-control" type="text" data-bind="value:ApplicantRoleName" readonly />
                                    </div>
                                    @*<div>
                                            <div class="col-sm-1">
                                                <label class="control-label">Relationship</label>
                                            </div>
                                            <div class="col-sm-2 no-padding">
                                                <input class="form-control" type="text" data-bind="value:RelationshipWithApplicantName" readonly />
                                            </div>
                                        </div>*@
                                    <div class="col-sm-3" data-bind="visible:ApplicantRole() === 2">
                                        <label class="control-label">Relationship With Applicant</label>
                                    </div>
                                    <div class="col-sm-3 no-padding" data-bind="visible:ApplicantRole() === 2">
                                        <select data-bind="options:$root.RelationshipsWithApplicant, value: RelationshipWithApplicant, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'" class="form-control"></select>
                                    </div>
                                </div>
                                <div class="form-group row row_design_detail">
                                    <div class="col-sm-1">
                                        <label class="control-label">Age</label>
                                    </div>
                                    @*<div class="col-sm-1 no-padding">
                                            <input class="form-control" type="number" min="0" data-bind="value:Age" />
                                        </div>*@

                                    <div class="col-sm-1 input-group" style="float:left !important;">
                                        <input class="form-control" type="number" min="0" data-bind="value:AgeInYears, valueUpdate:'keyup'" />
                                        <div class="input-group-addon">
                                            Y
                                        </div>
                                    </div>
                                    <div class="col-sm-1 input-group" style="float:left !important;">
                                        <input class="form-control" type="number" min="0" data-bind="value:AgeInMonths, valueUpdate:'keyup'" />
                                        <div class="input-group-addon">
                                            M
                                        </div>
                                    </div>
                                    <div class="col-sm-1">
                                        <label class="control-label" for="ProfessionName">Profession</label>
                                    </div>
                                    <div class="col-sm-2  no-padding">
                                        <input class="form-control" type="text" data-bind="value:ProfessionName" />
                                    </div>

                                    <div class="col-sm-1">
                                        <label class="control-label" for="Designation">Designation</label>
                                    </div>
                                    <div class="col-sm-2  no-padding">
                                        <input class="form-control" type="text" data-bind="value:Designation" />
                                    </div>
                                    <div class="col-sm-1">
                                        <label class="control-label" for="OrganizationName">Office</label>
                                    </div>
                                    <div class="col-sm-2 no-padding">
                                        <input class="form-control" type="text" data-bind="value:OrganizationName" />
                                    </div>

                                </div>
                                <div class="form-group row row_design_detail">
                                    <div class="col-sm-1">
                                        <label class="control-label" for="NID">NID</label>
                                    </div>
                                    <div class="col-sm-2 no-padding">
                                        <input class="form-control" type="number" data-bind="value:NID" />
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="row">
                                            <div class="col-sm-4">
                                                <label class="control-label">Date of Birth</label>
                                            </div>
                                            <div class="col-sm-8 input-group" data-bind="validationElement:DateOfBirth">
                                                <input type="datetime" data-bind="date:DateOfBirth" class="form-control" />
                                                <span class="help-block" style="display: none;"></span>
                                                <div class="input-group-addon">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="row">
                                            <div class="col-sm-4">
                                                <label class="control-label">Residence Status</label>
                                            </div>
                                            <div class="col-sm-8 no-padding" data-bind="validationElement:ResidenceStatus">
                                                <select class="form-control" data-bind="options:$parent.HomeOwnerships,optionsValue: 'Id', optionsText:'Name',optionsCaption:'--Select--',value:ResidenceStatus "></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group row row_design_detail">
                                    <div class="col-sm-6 no-right-padding">
                                        <div class="panel-bodycontent">
                                            <header class="panel-heading bodytitle pointer address" data-toggle="collapse">
                                                <h2 class="panel-title font">
                                                    <b>Present Address</b>
                                                </h2>
                                            </header>
                                            <div class="panel-body collapse presentAddress">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">line 1</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <input type="text" data-bind="value:PresentAddress.AddressLine1" class="form-control" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">line 2</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <input type="text" data-bind="value:PresentAddress.AddressLine2" class="form-control" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">line 3</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <input type="text" data-bind="value:PresentAddress.AddressLine3" class="form-control" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">Phone</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <input type="text" data-bind="value:PresentAddress.PhoneNo" class="form-control" />
                                                            </div>

                                                        </div>
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">Mobile</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <input type="text" data-bind="value:PresentAddress.CellPhoneNo" class="form-control" />
                                                            </div>

                                                        </div>

                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-4">
                                                                <label class="control-label">Postal Code</label>
                                                            </div>
                                                            <div class="col-sm-8">
                                                                <input type="number" data-bind="value:PresentAddress.PostalCode" class="form-control" />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-sm-6">
                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">Country</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <select data-bind="options:$root.CountryList, value: PresentAddress.CountryId, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'Name'" class="form-control"></select>
                                                            </div>
                                                        </div>

                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">Division</label>
                                                            </div>
                                                            <div class="col-sm-9">
                                                                <select data-bind="options:PresentAddress.DivisionList, value: PresentAddress.DivisionId, optionsCaption:'Choose One..', optionsValue:'Id', optionsText: 'DivisionNameEng'" class="form-control"></select>
                                                            </div>
                                                        </div>

                                                        <div class="form-group row row_design">
                                                            <div class="col-sm-3">
                                                                <label class="control-label">District</label>
