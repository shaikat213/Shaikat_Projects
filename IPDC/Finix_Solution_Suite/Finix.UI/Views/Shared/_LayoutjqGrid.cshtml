    <script src="~/Scripts/knockout.validation.js" type="text/javascript"></script>

    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/knockout-date-bindings.js"></script>
    <script src="~/Scripts/App/util.js" type="text/javascript"></script>

